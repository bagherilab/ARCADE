<parameters>
    <!-- default parameters for series -->
    <default id="RADIUS" value="1" />
    <default id="DEPTH" value="1" />
    
    <!-- default parameters for patch -->
    <patch id="GEOMETRY" value="hex" description="Patch geometry (hex or rect)" />
    
    <!-- default parameters for populations -->
    <population id="CELL_AGE_MIN" value="0" unit="min" description="0 weeks" />
    <population id="CELL_AGE_MAX" value="120960" unit="min" description="12 weeks" />
    <population id="CELL_VOLUME_MEAN" value="2250.0" unit="um^3" />
    <population id="CELL_VOLUME_STDEV" value="200.0" unit="um^3" />
    <population id="CELL_HEIGHT_MEAN" value="8.7" unit="um" />
    <population id="CELL_HEIGHT_STDEV" value="0" unit="um" />
    
    <!-- cellular decision making -->
    <population id="HETEROGENEITY" value="0.0" description="variation in cell agent parameters" />
    <population id="SENES_FRAC" value="0.5" description="fraction senescent vs. apoptotic" />
    <population id="NECRO_FRAC" value="0.5" description="fraction necrotic vs. apoptotic" />
    <population id="ENERGY_THRESHOLD" value="1000.0" description="maximum energy deficit" />
    <population id="DIVISION_POTENTIAL" value="50" description="max number of divisions" />
    <population id="COMPRESSION_TOLERANCE" value="0" units="um" description="maximum compression tolerance via height" />
    
    <!-- proliferation module parameters -->
    <population.module module="proliferation" id="SYNTHESIS_DURATION" value="637" units="min" description="duration of DNA synthesis" />
    
    <!-- migration module parameters -->
    <population.module module="migration" id="MIGRATION_RATE" value="0.24" unit="um/min" description="rate of cell migration" />
    
    <!-- apoptosis module parameters -->
    <population.module module="apoptosis" id="DEATH_DURATION" value="1080" unit="min" description="duration of cell death" />
     
    <!-- ACTIONS =========================================================== -->
    
    <!-- insert action parameters -->
    <action class="insert" id="TIME_DELAY" value="0" units="min" />
    <action class="insert" id="INSERT_RADIUS" value="1" />
    <action class="insert" id="INSERT_NUMBER" value="1" />
    
    <!-- remove action parameters -->
    <action class="remove" id="TIME_DELAY" value="0" units="min" />
    <action class="remove" id="REMOVE_RADIUS" value="1" units="min" />
     
    <!-- COMPONENTS ======================================================== -->
     
    <!-- source sites component parameters -->
    <component class="source_sites" id="X_SPACING" value="*" />
    <component class="source_sites" id="Y_SPACING" value="*" />
    <component class="source_sites" id="Z_SPACING" value="*" />
    <component class="source_sites" id="DAMAGE_SCALING" value="0" />
    
    <!-- pattern sites component parameters -->
    <component class="pattern_sites" id="RELATIVE_FRACTION" value="1.0" description="relative contribution of hemodynamic factors" />
    <component class="pattern_sites" id="WEIGHT_GRADIENT" value="10" description="weight of gradient hemodynamic factor" />
    <component class="pattern_sites" id="WEIGHT_LOCAL" value="10" description="weight of local hemodynamic factor" />
    <component class="pattern_sites" id="WEIGHT_FLOW" value="10" description="weight of flow hemodynamic factor" />
    <component class="pattern_sites" id="DAMAGE_SCALING" value="0" description="pattern site damage scaling" />
    
    <!-- graph site component parameters -->
    <component class="graph_sites" id="STEP_SIZE_XY" value="1" units="patch" conversion="DS" />
    <component class="graph_sites" id="STEP_SIZE_Z" value="1" units="patch" conversion="DZ" />
    <component class="graph_sites" id="LATTICE_SITE_VOLUME" value="1" units="patch" conversion="DS^2.DS" />
    <component class="graph_sites" id="GRAPH_LAYOUT" value="*" description="layout of graph sites" />
    <component class="graph_sites" id="ROOTS_LEFT" value="x" description="roots at the left of environment (x direction)" />
    <component class="graph_sites" id="ROOTS_RIGHT" value="x" description="roots at the right of environment (x direction)" />
    <component class="graph_sites" id="ROOTS_TOP" value="x" description="roots at the top of environment (y direction)" />
    <component class="graph_sites" id="ROOTS_BOTTOM" value="x" description="roots at the bottom of environment (y direction)" />
    <component class="graph_sites" id="OXYGEN_SOLUBILITY_TISSUE" value="0.00000131" units="fmol O2/(um^3 mmHg)" />
    <component class="graph_sites" id="OXYGEN_SOLUBILITY_PLASMA" value="0.00000138" units="fmol O2/(um^3 mmHg)" />
</parameters>
