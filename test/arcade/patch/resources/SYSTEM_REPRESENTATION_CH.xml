<set prefix="SYSTEM_REPRESENTATION_" path="/projects/p20519/abm/">
    <series name="CH_{dim_0}_{coord}" start="0" end="50" days="15">
        <simulation type="growth" name="{dim::(2D|3D)}" radius="{dim::(34|26)}" margin="6" height="{dim::(1|51)}">
            <profilers>
                <profiler type="growth" interval="720" suffix="" />
            </profilers>
        </simulation>
        <agents initialization="FULL">
            <populations>
                <population type="C" fraction="0.0">
                    <variables>
                        <variable id="max_height" scale="1.5" />
                        <variable id="meta_pref" scale="1.5" />
                        <variable id="migra_threshold" scale="0.5" />
                    </variables>
                </population>
                <population type="H" fraction="1.0" />
            </populations>
            <helpers>
                <helper type="insert" delay="1440" populations="0" bounds="0.05"/>
            </helpers>
        </agents>
        <environment coordinate="{coord::(hex|rect)}">
            <components>
                <component type="sites" class="source" />
            </components>
        </environment>
    </series>
</set>
