    <series name="both_C_C" start="0" end="20" days="15">
        <simulation type="growth">
            <profilers>
                <profiler type="growth" interval="720" suffix="" />
            </profilers>
        </simulation>
        <agents initialization="0">
            <populations>
                <population type="C" fraction="0.0">
                    <modules>
                        <module type="metabolism" version="C" />
                        <module type="signaling" version="C" />
                    </modules>
                </population>
            </populations>
            <helpers>
                <helper type="insert" delay="1440" populations="0" bounds="0.12"/>
