<set prefix="7_vascular_layouts_" path="/path/to/output/">
    <series name="L11" days="1">
        <simulation type="growth" radius="12" margin="2">
            <profilers>
                <profiler type="graph" interval="720" suffix=".GRAPH" />
            </profilers>
        </simulation>
        <agents initialization="4">
            <populations>
                <population type="C" fraction="1.0" />
            </populations>
        </agents>
        <environment coordinate="hex">
            <components>
                <component type="sites" class="graph">
                    <specifications>
                        <specification id="GRAPH_LAYOUT" value="L" />
                        <specification id="ROOTS_LEFT" value="20A80" />
                        <specification id="ROOTS_RIGHT" value="80V80" />
                        <specification id="ROOTS_TOP" value="x" />
                        <specification id="ROOTS_BOTTOM" value="x" />
                    </specifications>
                </component>
            </components>
        </environment>
    </series>
    <series name="S22" days="1">
        <simulation type="growth" radius="12" margin="2">
            <profilers>
                <profiler type="graph" interval="720" suffix=".GRAPH" />
            </profilers>
        </simulation>
        <agents initialization="4">
            <populations>
                <population type="C" fraction="1.0" />
            </populations>
        </agents>
        <environment coordinate="hex">
            <components>
                <components>
                <component type="sites" class="graph">
                    <specifications>
                        <specification id="GRAPH_LAYOUT" value="S" />
                        <specification id="ROOTS_LEFT" value="50A" />
                        <specification id="ROOTS_RIGHT" value="50A" />
                        <specification id="ROOTS_TOP" value="50V" />
                        <specification id="ROOTS_BOTTOM" value="50V" />
                    </specifications>
                </component>
            </components>
            </components>
        </environment>
    </series>
</set>