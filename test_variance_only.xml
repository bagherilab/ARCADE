<set prefix="combined_">
    <series name="grid" start="0" end="0" ticks="1" interval="1400" radius="10" margin="2" depth="1" varianceOnly="1">
        <patch>
            <patch.parameter id="GEOMETRY" value="hex" />
        </patch>
        <agents>
            <populations>
                <population id="cancerous" init="19" class="cancer">
                    <population.parameter id="CELL_VOLUME" value="NORMAL(MU=2250,SIGMA=200)" />
                    <population.parameter id="APOPTOSIS_AGE" value="NORMAL(MU=120960,SIGMA=10080)" />
                    <population.parameter id="NECROTIC_FRACTION" value="0.5" />
                    <population.parameter id="ACCURACY" value="0.8" />
                    <population.parameter id="AFFINITY" value="0.5" />
                    <population.parameter id="COMPRESSION_TOLERANCE" value="4.35" />

                    <population.parameter id="CELL_AGE" value="UNIFORM(MIN=0,MAX=0)" />
                    <population.process id="METABOLISM" version="complex" />
                    <population.process id="SIGNALING" version="complex"  />
                    <population.parameter id="proliferation/SYNTHESIS_DURATION" value="NORMAL(MU=637,SIGMA=20)" />
                    <population.parameter id="metabolism/BASAL_ENERGY" value="NORMAL(MU=0.001,SIGMA=0.00008)" />
                    <population.parameter id="metabolism/PROLIFERATION_ENERGY" value="NORMAL(MU=0.001,SIGMA=0.00008)" />
                    <population.parameter id="metabolism/MIGRATION_ENERGY" value="NORMAL(MU=0.0002,SIGMA=0.000016)" />
                    <population.parameter id="metabolism/METABOLIC_PREFERENCE" value="NORMAL(MU=0.3,SIGMA=0.024)"/>
                    <population.parameter id="metabolism/CONVERSION_FRACTION" value="NORMAL(MU=0.25,SIGMA=0.02)" />
                    <population.parameter id="metabolism/RATIO_GLUCOSE_PYRUVATE" value="NORMAL(MU=0.5,SIGMA=0.04)" />
                    <population.parameter id="metabolism/LACTATE_RATE" value="NORMAL(MU=0.1,SIGMA=0.008)" />
                    <population.parameter id="metabolism/AUTOPHAGY_RATE" value="NORMAL(MU=0.0001,SIGMA=0.000008)" />
                    <population.parameter id="metabolism/GLUCOSE_UPTAKE_RATE" value="NORMAL(MU=1.12,SIGMA=0.0896)" />
                    <population.parameter id="metabolism/ATP_PRODUCTION_RATE" value="NORMAL(MU=8.927,SIGMA=0.71416)" />
                    <population.parameter id="signaling/MIGRATORY_THRESHOLD" value="NORMAL(MU=10,SIGMA=1)"/>
                </population>
            </populations>
        </agents>
        <environments>
            <layers>
               <layer id="GLUCOSE">
                    <layer.parameter id="INITIAL_CONCENTRATION" value="0.005" />
                    <layer.parameter id="DIFFUSIVITY" operation="diffuser" value="39.0" />
                    <layer.parameter id="CONCENTRATION" operation="generator" value="0.005" units="fmol/um^3" description="equal to 5 umol/cm^3" />
                    <layer.operation id="GENERATOR" />
                    <layer.operation id="DIFFUSER" />
                </layer>
                <layer id="OXYGEN">
                    <layer.parameter id="INITIAL_CONCENTRATION" value="100" />
                    <layer.parameter id="DIFFUSIVITY" operation="diffuser" value="2000.0" />
                    <layer.parameter id="CONCENTRATION" operation="generator" value="100" units="mmHg" />
                    <layer.operation id="DIFFUSER" />
                    <layer.operation id="GENERATOR" />
                </layer>
                <layer id="TGFA">
                    <layer.parameter id="INITIAL_CONCENTRATION" value="150" />
                    <layer.parameter id="DIFFUSIVITY" operation="diffuser" value="51.8" />
                    <layer.operation id="DIFFUSER" />
                </layer>
            </layers>
            <components>
                <component id="SITES" class="source_sites">
                    <component.register id="GLUCOSE" />
                    <component.register id="OXYGEN" />
                    <component.parameter id="X_SPACING" value="*:2" />
                    <component.parameter id="Y_SPACING" value="*:2" />
                </component>
            </components>
        </environments>
    </series>
</set>
